{
  "name": "color-name",
  "version": "0.0.1",
  "description": "colorname.io",
  "main": "build/server.js",
  "scripts": {
    "webpack": "webpack --mode=production",
    "webpack:dev": "webpack --mode=development --watch",
    "node:dev": "nodemon build/server.js",
    "dev": "npm-run-all --parallel webpack:dev node:dev",
    "lint": "standard --env jasmine",
    "lint:fix": "standard --env jasmine --fix",
    "test": "babel-istanbul cover jasmine-node spec",
    "build": "yarn webpack",
    "docker:dev": "docker-compose build; docker-compose -f docker-compose.yml up -d",
    "docker:prod": "docker-compose build; docker-compose -f docker-compose.prod.yml up -d",
    "start": "node build/server.js"
  },
  "author": "Simon Breiter",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "create-react-class": "^15.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.15.4",
    "jasmine": "^2.7.0",
    "jasmine-node": "^1.14.5",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-motion": "^0.5.0",
    "react-motion-ui-pack": "^0.10.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "standard": "^10.0.3",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^1.1.11",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13"
  }
}
